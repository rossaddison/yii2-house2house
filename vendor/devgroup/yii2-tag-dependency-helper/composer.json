    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "~2.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\TagDependencyHelper\\": "src/"
